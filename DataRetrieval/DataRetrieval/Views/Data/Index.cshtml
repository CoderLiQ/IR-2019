@{
    ViewData["Title"] = "Data Page";
}

<div class="text-center">
    <form method="post" enctype="multipart/form-data" asp-controller="Data" asp-action="ImportCSV">
        <div class="form-group">
            <div class="col-md-10">
                <p>Upload csv:</p>
                <input type="file" name="file"/>
            </div>
        </div>
        <div class="form-group">
            <div class="col-md-10">
                <input type="submit" value="Upload"/>
            </div>
        </div>
    </form>
</div>

